
# jedi-vim
pip install jedi
git clone https://github.com/davidhalter/jedi-vim ~/.vim/bundle/jedi-vim

# tagbar
yum -y install ctags
git clone https://github.com/majutsushi/tagbar ~/.vim/bundle/tagbar

# auto-close
git clone https://github.com/Townk/vim-autoclose

# minibufexpl.vim
git clone https://github.com/fholgado/minibufexpl.vim

# auto format
pip install yapf
git clone https://github.com/Chiel92/vim-autoformat

# easymotion
git clone https://github.com/easymotion/vim-easymotion
