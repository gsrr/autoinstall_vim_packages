
# plugin:ctrlp.vim
git clone https://github.com/kien/ctrlp.vim ~/.vim/bundle/ctrlp.vim

# jedi-vim
pip install jedi
git clone https://github.com/davidhalter/jedi-vim ~/.vim/bundle/jedi-vim

# supertab
git clone https://github.com/ervandew/supertab ~/.vim/bundle/supertab

# syntastic
git clone https://github.com/scrooloose/syntastic ~/.vim/bundle/syntastic

# tagbar
yum -y install ctags
git clone https://github.com/majutsushi/tagbar ~/.vim/bundle/tagbar

# vim-airline
git clone https://github.com/vim-airline/vim-airline ~/.vim/bundle/vim-airline

# vim-fugitive
git clone https://github.com/tpope/vim-fugitive ~/.vim/bundle/vim-fugitive

# auto-close
git clone https://github.com/Townk/vim-autoclose

# minibufexpl.vim
git clone https://github.com/fholgado/minibufexpl.vim

# snipmate
